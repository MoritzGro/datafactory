{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory-Name",
			"defaultValue": "datafactorymoritz"
		},
		"moritzdatalake2_accountKey": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"accountKey\" von \"moritzdatalake2\""
		},
		"datamoritz_connectionString": {
			"type": "secureString",
			"metadata": "Sichere Zeichenfolge für \"connectionString\" von \"datamoritz\""
		},
		"moritzdatalake2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://moritzdatalake2.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI für Speicherkonto mit verknüpften ARM-Vorlagen"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "SAS-Token für Speicherkonto mit verknüpften ARM-Vorlagen"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorymoritz_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"moritzdatalake2_accountKey": {
						"value": "[parameters('moritzdatalake2_accountKey')]"
					},
					"datamoritz_connectionString": {
						"value": "[parameters('datamoritz_connectionString')]"
					},
					"moritzdatalake2_properties_typeProperties_url": {
						"value": "[parameters('moritzdatalake2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}